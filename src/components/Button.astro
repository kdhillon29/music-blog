---
const { title } = Astro.props;
---

<button
  id="contact-btn"
  class="bg-teal-900 text-white p-2 rounded-3xl border-2 border-teal-900"
  >{title}</button
>

<script>
  const button = document.getElementById("contact-btn");
  button?.addEventListener("click", () => {
    alert("Thanks for reaching out!");
  });
</script>
